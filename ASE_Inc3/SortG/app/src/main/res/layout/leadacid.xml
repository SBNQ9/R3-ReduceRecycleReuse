<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#ffffffff">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LEAD ACID BATTERIES"
        android:id="@+id/textView10"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:textStyle="bold|italic"
        android:textSize="25dp" />

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="300dp"
        android:id="@+id/imageView5"
        android:src="@drawable/labat"
        android:layout_below="@+id/textView10"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Lead acid batteries can be found in fork lift trucks and in UPS. Deposit them at your local recycling park. Companies using large numbers of these batteries can register themselves as a Bebat collection point. Bebat will collect the batteries at no charge."
        android:id="@+id/textView11"
        android:textStyle="normal"
        android:textSize="20dp"
        android:background="#ffffffff"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="67dp" />
</RelativeLayout>
