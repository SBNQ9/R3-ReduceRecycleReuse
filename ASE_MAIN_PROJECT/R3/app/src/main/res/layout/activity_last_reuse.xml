<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.boss.r3.LastReuse"
    >

    <TextView android:text="YOU CAN CONTACT THE NUMBER BESIDES THE IMAGE FOR ITEM" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView64" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GO TO LIST VIEW ITEMS"
        android:id="@+id/red1"
        android:layout_marginTop="88dp"
        android:layout_marginStart="46dp"
        android:layout_below="@+id/textView64"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GO TO WELCOME PAGE"
        android:id="@+id/red2"
        android:layout_below="@+id/red1"
        android:layout_alignStart="@+id/red1" />

</RelativeLayout>
