<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.boss.sortg.list"
    android:clickable="true">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="                   SORTING GUIDE"
        android:id="@+id/textView2"
        android:textStyle="bold|italic"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/i1"
        android:layout_marginTop="34dp"
        android:scaleType="fitXY"
        android:background="#ffffffff"
        android:src="@drawable/i1"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true"
        android:padding="5dp"
        android:clickable="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/i2"
        android:scaleType="fitXY"
        android:background="#ffffffff"
        android:src="@drawable/i2"
        android:layout_alignTop="@+id/i1"
        android:layout_centerHorizontal="true"
        android:padding="5dp"
        android:clickable="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/i3"
        android:scaleType="fitXY"
        android:background="#ffffffff"
        android:src="@drawable/i3"
        android:layout_alignTop="@+id/i2"
        android:layout_alignParentEnd="true"
        android:padding="5dp"
        android:clickable="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/i4"
        android:scaleType="fitXY"
        android:background="#ffffffff"
        android:src="@drawable/i4"
        android:padding="5dp"
        android:layout_below="@+id/i1"
        android:layout_alignParentStart="true"
        android:clickable="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/i5"
        android:scaleType="fitXY"
        android:background="#ffffffff"
        android:src="@drawable/i5"
        android:padding="5dp"
        android:layout_below="@+id/i2"
        android:layout_centerHorizontal="true"
        android:clickable="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/i6"
        android:scaleType="fitXY"
        android:background="#ffffffff"
        android:src="@drawable/i6"
        android:layout_below="@+id/i3"
        android:layout_alignStart="@+id/i3"
        android:padding="5dp"
        android:clickable="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/i7"
        android:scaleType="fitXY"
        android:background="#ffffffff"
        android:src="@drawable/i7"
        android:padding="5dp"
        android:layout_below="@+id/i4"
        android:layout_alignParentStart="true"
        android:clickable="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/i8"
        android:scaleType="fitXY"
        android:background="#ffffffff"
        android:src="@drawable/i8"
        android:padding="5dp"
        android:layout_below="@+id/i5"
        android:layout_centerHorizontal="true"
        android:clickable="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/i9"
        android:scaleType="fitXY"
        android:background="#ffffffff"
        android:src="@drawable/i9"
        android:padding="5dp"
        android:layout_below="@+id/i6"
        android:layout_alignParentEnd="true"
        android:clickable="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/i10"
        android:scaleType="fitXY"
        android:background="#ffffffff"
        android:src="@drawable/i10"
        android:padding="5dp"
        android:layout_below="@+id/i7"
        android:layout_alignParentStart="true"
        android:clickable="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/i11"
        android:scaleType="fitXY"
        android:background="#ffffffff"
        android:src="@drawable/i11"
        android:padding="5dp"
        android:layout_below="@+id/i8"
        android:layout_centerHorizontal="true"
        android:clickable="true" />

    <ImageButton
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:id="@+id/i12"
        android:scaleType="fitXY"
        android:background="#ffffffff"
        android:src="@drawable/i12"
        android:padding="5dp"
        android:layout_below="@+id/i9"
        android:layout_alignStart="@+id/i9"
        android:clickable="true" />


</RelativeLayout>
